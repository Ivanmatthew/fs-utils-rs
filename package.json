{
  "name": "@ivanmatthew/fs-utils-rs",
  "version": "0.1.1",
  "description": "A rustified utility library for managing filesystem-heavy operations",
  "repository": "git@github.com/Ivanmatthew/fs-utils-rs.git",
  "author": "IvanMatthew",
  "keywords": [
    "filesystem",
    "utility",
    "rust",
    "node",
    "napi"
  ],
  "main": "index.js",
  "types": "index.d.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "napi": {
    "name": "fs-utils-rs",
    "triples": {
      "additional": [
        "universal-apple-darwin"
      ]
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "^2.18.3",
    "ava": "^6.0.1"
  },
  "ava": {
    "timeout": "3m"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "ava",
    "universal": "napi universal",
    "version": "napi version"
  },
  "packageManager": "yarn@4.2.2",
  "optionalDependencies": {
    "@ivanmatthew/fs-utils-rs-win32-x64-msvc": "0.1.1",
    "@ivanmatthew/fs-utils-rs-darwin-x64": "0.1.1",
    "@ivanmatthew/fs-utils-rs-linux-x64-gnu": "0.1.1",
    "@ivanmatthew/fs-utils-rs-darwin-universal": "0.1.1"
  }
}